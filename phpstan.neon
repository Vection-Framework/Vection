includes:
    - phpstan-baseline.neon
parameters:
    level: 5
    paths:
        - src
    inferPrivatePropertyTypeFromConstructor: true
    excludes_analyse:
        - %rootDir%/../../../src/Component/*/Tests/*
        - %rootDir%/../../../src/Component/Http/*
        - %rootDir%/../../../src/Bridge/*
        - %rootDir%/../../../src/Component/Cache/Provider/APCuCacheProvider.php
        - %rootDir%/../../../src/Component/Database/Database.php