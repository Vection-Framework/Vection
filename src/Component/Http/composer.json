{
    "name": "vection-framework/http",
    "type": "library",
    "description": "PHP HTTP Kernel / Processor with middleware support - Vection Framework Component",
    "keywords": [
        "http", "php-http-kernel", "request", "response", "http-middleware", "http-stack"
    ],
    "homepage": "https://vection.appsdock.org",
    "license": "MIT",
    "authors": [
        {
            "name": "David M. Lung",
            "email": "david.lung@appsdock.de"
        },
        {
            "name": "David Brucksch",
            "email": "david.brucksch@appsdock.de"
        }
    ],
    "require": {
        "php": "^8.1",
        "vection-framework/common": "v0.*",
        "vection-framework/contracts": "v0.*",
        "psr/http-message": "1.*",
        "psr/http-server-handler": "1.*",
        "psr/http-client": "1.0.*",
        "psr/log": "1.*",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Vection\\Component\\Http\\": "./"
        }
    },
    "minimum-stability": "stable"
}
