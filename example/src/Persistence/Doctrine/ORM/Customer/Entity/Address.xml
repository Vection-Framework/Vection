<doctrine-mapping
        xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        https://raw.githubusercontent.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity
            name="Example\Domain\Customer\Entity\Address"
            table="address"
            schema="parking">

        <id name="id" type="integer" column="address_id">
            <generator/>
        </id>

        <field name="street" column="address_street"/>
        <field name="postalCode" column="address_postal_code"/>
        <field name="city" column="address_city"/>
        <field name="country" column="address_country"/>
        <field name="type" column="address_type"/>
    </entity>

</doctrine-mapping>